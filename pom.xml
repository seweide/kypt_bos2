            <dependency>
                <groupId>net.liangyihui.kypt.manager</groupId>
                <artifactId>kypt-bos-api</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>net.liangyihui.kypt.manager</groupId>
