        <module>kypt-bos-base</module>
        <module>kypt-bos-core</module>
    </modules>


    <properties>
        <dmp.common.version>1.0.7</dmp.common.version>
        <commons.collections.version>3.2.2</commons.collections.version>
        <std.web.common.version>1.0.0-SNAPSHOT</std.web.common.version>
        <baomidou.mybatis.version>3.3.1.tmp</baomidou.mybatis.version>
